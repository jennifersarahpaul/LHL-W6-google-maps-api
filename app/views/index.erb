<!DOCTYPE html> 
<html>
  <head>
    <style type="text/css">
      html, body, #map
      {
        height: 100%;
        width: 100%;
        margin: 0;
        padding: 0;
      }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script type="text/javascript">
      /* globals google */
      var map; 
      function initMap() { //jshint ignore:line
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 48.535, lng: -123.351}, 
          zoom: 10
        });
      }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyASCcBj5Y3xxIFhYBvPxLMO3hSwpDTgF8k&amp;callback=initMap">
    </script>
  </body>
</html>
